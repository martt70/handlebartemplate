<!--<h2>Auxilliary Boiler</h2>-->
<div id="gigs">
<h1>{{equipment_area}}</h1>
</div>
<section id="gigs" class="container"></section>
{{#each gigs}}
<!--<h1>All Gigs</h1>-->

<div class="gig">
      <h3>{{title}}</h3>
      <!--<p>{{manufacturer}}</p>-->
      <ul>
        <!--<li>Budget: {{budget}}</li>-->
        <!--<li><a href="mailto:{{contact_email}}" class="btn btn-reverse">Apply Now</a></li>-->
        <h3>{{equipment_area}}</h3>
        <li>photo:{{photo_image}}</li>
      
         
        <!--<img id="img" class="std" $("img.std").toggleClass("img.std img.big") src="/uploads/{{photo_image}}" width="70" height="70">;-->
        <!--<img id="img" class="std" $("this.className=='std'?this.className='big':this.className='std'" src="/uploads/{{photo_image}}).toggle()">-->
        <!--<img id="img" class="std" onClick="(this.className=='std')?this.className='big':this.className='std'" src="https://cdn.experts-exchange.com/images/experts-exchange/header2017Q4/expertsExchangeLogo.svg" >-->
        <img class="ex1" src="/uploads/{{photo_image}}" alt="" width="70" height="70"/>
        <!--<img id="img" src="/uploads/{{photo_image}}" width="70" height="70">-->
        <li>Equipment Type: {{equipment_type}}</li>
        <li>Manufacturer: {{manufacturer}}</li>
        <li>Model Number: {{model_number}}</li>
        <li>Serial Number: {{serial_number}}</li>
        <li>Condition: {{severity}}</li>
        <li>Comments: {{general_comments}}</li>

                
      </ul>
      <!--
      <div class="tech">
        <small>Technologies Needed: <span>{{technologies}}</span></small>
      </div>
      -->
    </div>
    {{else}}
     <p>No records available</p>
    {{/each}}
</section>

